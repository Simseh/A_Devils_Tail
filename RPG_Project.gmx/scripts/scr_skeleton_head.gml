switch(face){
    case RIGHT:
        sprite_index = spr_skeleton_head_right;
        break;
    case UP:
        sprite_index = spr_skeleton_head_up_down;
        break;
    case LEFT:
        sprite_index = spr_skeleton_head_left;
        break;
    case DOWN:
        sprite_index = spr_skeleton_head_up_down;
        break;
}
